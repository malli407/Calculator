pipeline {
    agent any
    environment{
       PATH="/usr/bin/apache-maven-3.6.3"
       }
    stages {
        stage("git checkout"){
            steps {
                git credentialsId: 'Github', url: 'https://github.com/malli407/Calculator'
                echo "This is Mallikarjuna Reddy"
                }
                }
        stage("maven build"){
            steps{
                sh "maven clean package"
            }
        }        
                
               
            
        
    }
}
